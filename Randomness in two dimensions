import numpy as n
pimport matplotlib.pyplot as plt

def randrange(n, vmin, vmax):
    return (vmax - vmin)*np.random.rand(n) + vmin
    
fig = plt.figure()
ax = fig.add_subplot(111)
n = 100
for c, m in [('r', 'o'), ('b', '^')]:
    xs = randrange(n, -5, 5)
    ys = randrange(n, -5, 5)
    ax.scatter(xs, ys,Â  c=c, marker=m)

ax.set_xlabel('X Label')
ax.set_ylabel('Y Label')

plt.show()
